{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es2016",
    "moduleResolution": "node",
    "outDir": "dist",
    "baseUrl": "./",
    "sourceMap": true,
    "downlevelIteration": true,
    "noImplicitAny": false,

    "paths": {
      "*": ["node_modules/*"]
    }
    // "forceConsistentCasingInFileNames": true ,
    // "strict": true,
    // "skipLibCheck": true,
  },
  "include": ["src/**/*"]
}
